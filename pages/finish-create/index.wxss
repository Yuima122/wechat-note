/* pages/finish-create/index.wxss */
page{
    width: 100%;
    height: 100%;
}
.container{
    display: flex;
    justify-content: center;
    width: 100%;
    height: 100%;
    background-color: #fdd32a;
}
.finish-text{
    position: absolute;
    top: 90rpx;
    font-family: PingFang SC;
    font-size: 48rpx;
    font-weight: 900;
    color: #404040;
}
.invite{
    position: absolute;
    top: 630rpx;
    display: flex;
    justify-content: center;
    width: 436rpx;
    height: 96rpx;
    border-radius: 16rpx;
    box-shadow: 0 16rpx 36rpx 0 rgba(214, 86, 46, 0.2);
    background-color: #ffffff;
    font-family: PingFang SC;
    font-size: 36rpx;
    font-weight: bold;
    color: rgba(0, 0, 0, 0.75);
}
.cancel{
    position: absolute;
    top: 768rpx;
    font-family: PingFang SC;
    font-size: 36rpx;
    font-weight: bold;
    color: #ffffff;
}